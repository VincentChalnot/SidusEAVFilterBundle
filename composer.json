{
    "name": "sidus/eav-filter-bundle",
    "description": "Extension for sidus/eav-model-bundle and sidus/filter-bundle to handle the EAV model with a fallback to elastic search",
    "keywords": [
        "eav",
        "elastica",
        "elasticsearch",
        "filter",
        "doctrine",
        "sort",
        "datagrid"
    ],
    "homepage": "https://github.com/VincentChalnot/SidusEAVFilterBundle",
    "type": "library",
    "license": "GPL-2.0",
    "authors": [
        {
            "name": "Vincent Chalnot",
            "email": "vincent@sidus.fr",
            "homepage": "http://chalnot.fr",
            "role": "Developer"
        }
    ],
    "target-dir": "Sidus/EAVFilterBundle",
    "autoload": {
        "psr-0": {
            "Sidus\\EAVFilterBundle": ""
        }
    },
    "require": {
        "sidus/eav-model-bundle": "dev-v1.2.x-dev",
        "sidus/filter-bundle": "dev-v1.2.x-dev",
        "friendsofsymfony/elastica-bundle": "*"
    }
}
